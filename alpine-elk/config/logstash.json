input {
  stdin { }
  http {
    port => [8080,80]
  }
  tcp {
    port => 514
    type => syslog
  }
  udp {
    port => 514
    type => syslog
  }
  file{
    type => "apache"
    path => "/var/lib/elasticsearch/elasticsearch.log"
  }
}

output {
  elasticsearch { host => localhost }
  stdout { codec => rubydebug }
}
